{
    "env": {
        "es2021": true
    },
    "extends": [
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "indent": ["error", 4],
        "object-curly-spacing": [2, "always"],
        "require-jsdoc": 0,
        "max-len": 0
    }
}
